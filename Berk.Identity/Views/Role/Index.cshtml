@model List<AppRole>

@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}


<a asp-action="Create" asp-controller="Role" class="d-inline-block my-2">Yeni Rol Ekle</a>
<table class="table table-hovered table-bordered table-sm">

	<thead>
		<tr>
			<th>Name</th>
			<th>Created Time</th>
			

		</tr>
	</thead>



	<tbody>
		@foreach (var item in Model)
		{


			<tr>
				<td>@item.Name</td>
				<td>@item.CretedTime</td>
				


			</tr>

		}



	</tbody>

</table>



